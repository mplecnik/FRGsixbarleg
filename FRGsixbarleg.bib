@article{erdmanGeneralMethodKinetoElastodynamic1972,
  title = {A {{General Method}} for {{Kineto}}-{{Elastodynamic Analysis}} and {{Synthesis}} of {{Mechanisms}}},
  volume = {94},
  issn = {1087-1357},
  doi = {10.1115/1.3428335},
  abstract = {Kineto-elastodynamics is the study of the motion of mechanisms consisting of elements which may deflect due to external loads or internal body forces. This paper describes the initial phases in the development of a general method of kineto-elastodynamic analysis and synthesis based on the flexibility approach of structural analysis, which may be applied to any planar or spatial mechanism. Dynamic error is investigated due to flexural, longitudinal, and torsional element strain, and system inertia fluctuations; the treatment of Coulomb and viscous friction is indicated. Kineto-Elastodynamic Stretch Rotation Operators are derived which will rotate and stretch both planar and spatial link vectors reflecting rigid body motion plus elastic deformations of the link. A numerical example is presented to demonstrate the elastodynamic analysis technique.},
  number = {4},
  journal = {Journal of Engineering for Industry},
  author = {Erdman, A. G. and Sandor, G. N. and Oakberg, R. G.},
  month = nov,
  year = {1972},
  pages = {1193-1205},
  file = {E:\\Dropbox\\Publications\\Zotero\\storage\\RQDSGVYB\\Erdman et al. - 1972 - A General Method for Kineto-Elastodynamic Analysis.pdf}
}

@article{skreinerDynamicAnalysisUsed1970,
  title = {Dynamic Analysis Used to Complete the Design of a Mechanism},
  volume = {5},
  issn = {0022-2569},
  doi = {10.1016/0022-2569(70)90054-6},
  abstract = {The design process is briefly discussed under seven headings. This is followed by methods to analyze a linkage mechanism on a static and dynamic basis. A dynamic motion analysis is performed, and pin forces are calculated for the actual conditions of velocity and acceleration. The methods are illustrated throughout by a design example. It is shown that the usual assumption of constant speed at some reference link does not give accurate values of the pin forces. The concept of the transmission angle, used as a criterion to assess a linkage based on the geometry, is supplemented by a new factor which allows the dynamic performance to be appraised. A spring is used to improve the design based on fluctuation of speed and pin forces. A process of optimisation is discussed to synthesize further, or tune the mechanism.
Zusammenfassung
Anwendung der Dynamischen Analyse zur Vervollst\"andigung des Entwurfes eines Getriebes; Dr. M. Skreiner. Der Entwurfsprozess ist kurz besprochen unter sieben Titeln. Dieses ist erweitert durch Methoden der Analyse eines Getriebes auf statischer und dynamischer Grundlagen. Eine dynamische Bewegungsanalyse ist durchgef\"uhrt und die Stiftkr\"afte sind berechnet auf Grund der tats\"achlichen Geschwindigkeits und Beschleunigungs-Zust\"ande. Die Methoden sind durch ein Beispiel erl\"autert. Es wird gezeigt, dass die \"ubliche Annahme einer best\"andigen Geschwindigkeit f\"ur ein gewisses Glied nicht die genauen Gr\"ossen der Stiftk\"afte liefert. Der Begriff des \"Ubertragungswinkels, als Kennzeichen zur Absch\"atzung eines Getriebes auf Grund der Geometrie, ist durch einen neuen Faktor erg\"anzt, welcher die Absch\"atzung der dynamischen Ausf\"uhrung erm\"oglicht. Eine Feder ist angewandt zur Verbesserung des Entwurfes auf Grund des Schwankens der Geschwindigkeit und der Stiftkr\"afte. Ein Verfahren zur Optimisierung ist besprochen f\"ur weitere Synthese oder Abstimmungdes Getriebes.
